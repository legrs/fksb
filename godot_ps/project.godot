; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PhysicsSimulate"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[debug_draw_3d]

settings/addon_root_folder="res://addons/debug_draw_3d"

[dotnet]

project/assembly_name="PhysicsSimulate"

[filesystem]

import/blender/enabled=false

[physics]

3d/run_on_separate_thread=true
3d/physics_engine="Jolt Physics"
jolt_physics_3d/simulation/allow_sleep=false
jolt_physics_3d/simulation/sleep_velocity_threshold=0.0
jolt_physics_3d/simulation/sleep_time_threshold=0.0
jolt_physics_3d/limits/max_linear_velocity=10000.0
jolt_physics_3d/limits/max_angular_velocity=471.23889803846896
jolt_physics_3d/limits/max_bodies=256
jolt_physics_3d/limits/max_body_pairs=256
jolt_physics_3d/limits/max_contact_constraints=256
3d/default_gravity=9806.6
3d/default_gravity_vector=Vector3(0, 0, -1)
3d/default_linear_damp=0.0
3d/default_angular_damp=0.0
3d/sleep_threshold_linear=0.0
3d/sleep_threshold_angular=0.0
3d/time_before_sleep=0.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
